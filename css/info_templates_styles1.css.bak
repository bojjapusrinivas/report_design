@charset "utf-8";
/* CSS Document */

/*===================
	BODY STYLES
====================*/
body{
	
	padding-top:10px !important;
}

/*===================
	SNIPTS STYLES
====================*/

.mx-auto{
	margin:0 auto !important;	
}

.count{
	color:#236EB6;
	font-weight:bold;
	font-style:italic;	
}

.info_template_report{
	width:100%;
	min-height:550px;
	background:url(/images/001.jpg) no-repeat;
	background-size:cover;
	position:relative;
	box-shadow:0px 0px 20px rgba(0,0,0,0.2);
	border:rgba(255,255,255,0.5) solid 1px;
	
	box-sizing:border-box;
}

.cover{
	position:absolute;
	width:100%;
	height:100%;
	background:rgba(194,223,249,0.8);	
}


.block_relative{
	position:relative;	
}

.content{
	height:526px;
	margin-top:10px;
	overflow:hidden;
}

.skew_desing{
	position:absolute;
	width:100%;
	height:100%;
	left:5%;
	top:0%;
	background:rgba(255,255,255,0.95);
	transform: skewx(-12deg);
	margin-left: -14%;
	padding:25px;
	box-sizing:border-box;
	border: #0071BC solid 3px;
}

.skew_desing:before{
	content: "";
    position: absolute;
    width: 3px;
    height: 100%;
    left: 8.6%;
    top: 0%;
    background: #0071BC;
    transform: skewx(12deg);
}

.skew_desing .eleven{
transform: skewx(12deg);
margin-left: 30px !important;

}

.skew_desing .five{
	text-align:center;
	padding:5px;
    box-sizing:border-box;
	margin-bottom:17px;
}

.info{
	width:110px;
	height: 110px;
	border:#EA0A0A solid 6px;
	border-radius:100%;
	margin:0 auto;
	padding:10px;
	box-sizing:border-box;
	margin-bottom:55px;
	position:relative;
	box-shadow:0px 0px 30px rgba(0,0,0,0.3);
	transistion: all 0.2s linear;
	-webkit-transistion: all 0.2s linear;
	-moz-transistion: all 0.2s linear;

}

.info:before{
	content: "";
    width: 8px;
    height: 40px;
    position: absolute;
    background: #EA0A0A;
    bottom: -40px;
    left: 48%;;	
	
}

.info:after {
    content: "";
    width: 15px;
    height: 15px;
    position: absolute;
    background: #fff;
    border: #EA0A0A solid 4px;
    bottom: -59px;
    left: 41%;
    border-radius: 100%;
}

.info:hover{
	box-shadow:0px 0px 30px rgba(0,0,0,0.0);
	transistion: all 0.2s linear;
	-webkit-transistion: all 0.2s linear;
	-moz-transistion: all 0.2s linear;
}

.skew_desing .five h4{
	font-style:italic;
}

.company_logo_block {
	width: 250px;
    height: 250px;
    margin-top: 10px;
    border: #0071BC solid 3px;
    background:url(images/001.jpg);
    z-index: 999;
    position: absolute;
    border-radius: 100%;
    left: -180px;
    margin-top: 125px;
	background-size: 500%;
    background-position: 73%;
	position:relative;
}

.company_logo_block .cover{
	border-radius:100%;	
	z-index:-1
}
	

   .comp_logo {
    width: 95%;
    height: 95%;
    background: #fff;
    border-radius: 100%;
    z-index: 99999;
    margin: 0 auto;
    margin-top: 2%;
	padding-top:40px;
	box-sizing:border-box;
}


	.comp_logo img {
    width: 150px;
    height: auto;
    margin: 0 auto;
    margin-top: 50px;
}
	


