    $(function() {
        var popup = $('#autoload');
        var time = $(".will-close strong");
        var closeSeconds = $("#autoload").attr("data-close");
        var openSeconds = $("#autoload").attr("data-open");

        setTimeout(function(e) {

            popup.modal('show');
            time.html(closeSeconds);

            setInterval(function() {
                time.html(closeSeconds);
                closeSeconds--;
                if (closeSeconds < 0) {
                    popup.modal('hide');
                }

            }, 1000)

        }, openSeconds * 100);
    });

    $('#autoload').modal({
        backdrop: 'static',
        keyboard: false
    })

